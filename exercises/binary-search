#include<iostream>

using namespace std;

int main(){
vector<int>A;
int B; //number to search
int n;   //number of elements in array
int i,high,low,mid;
int result,flag=0;
cout<<"enter the number you want to search"<<endl;
cin>>B;
for(i=0;i<n;i++)
{
cin>>A[i];
}
sort(A.begin(),A.end());
high=n-1;
low=0;

while(high>=low){
mid=high+(low-high)/2;

if(A[mid]==B)
{
result=mid;
flag=1;
}

else if(A[mid]>B)
high=mid-1;

else
low=mid+1;
}

if(flag==1)
cout<<"number found at index number "<<result<<endl;

else
cout<<"number not found"<<endl;

return 0;
}




